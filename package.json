{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run dev-python\"",
    "dev-python": "python ./src/utils/generate_feedback.py",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate": "openapi-generator-cli generate -i http://127.0.0.1:8000/openapi.json -g typescript-fetch -o src/generated --additional-properties=\"legacyDiscriminatorBehavior=true\""
  },
  "dependencies": {
    "axios": "^1.7.9",
    "openai": "^4.80.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vuetify": "^3.7.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/axios": "^0.9.36",
    "@types/node": "^20.17.14",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^6.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.4.14",
    "vue-tsc": "^2.0.21"
  }
}
